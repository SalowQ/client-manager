{
  "buildCommand": "npm run install:all && chmod +x build.sh && ./build.sh",
  "outputDirectory": "apps/host/dist",
  "installCommand": "npm install",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/auth",
      "destination": "/auth/"
    },
    {
      "source": "/auth/(.*)",
      "destination": "/auth/$1"
    },
    {
      "source": "/clients",
      "destination": "/clients/"
    },
    {
      "source": "/clients/(.*)",
      "destination": "/clients/$1"
    },
    {
      "source": "/ui/(.*)",
      "destination": "/ui/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/$1"
    }
  ]
}
